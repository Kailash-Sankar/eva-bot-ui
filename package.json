{
  "name": "eva-bot-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && rm -rf out/ && npm install && npm run dev",
    "compile-sass": "node-sass assets/scss/dashboard.scss assets/css/dashboard.css",
    "minify-sass": "node-sass assets/scss/dashboard.scss assets/css/dashboard.min.css --output-style compressed",
    "map-sass": "node-sass assets/scss/dashboard.scss assets/css/dashboard.css --source-map true",
    "all-sass": "npm run compile-sass && npm run minify-sass && npm run map-sass",
    "dev": "next dev"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.14.0",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-sass": "1.0.1",
    "apollo-boost": "^0.4.9",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "date-fns": "^2.0.0-beta.5",
    "graphql": "^15.4.0",
    "moment": "^2.29.1",
    "next": "9.5.2",
    "next-compose-plugins": "2.2.0",
    "next-fonts": "1.4.0",
    "next-images": "1.3.1",
    "node-sass": "4.14.1",
    "path": "0.12.7",
    "react": "16.13.1",
    "react-apollo": "^3.1.5",
    "react-chartjs-2": "2.10.0",
    "react-copy-to-clipboard": "5.0.2",
    "react-date-range": "^1.1.3",
    "react-dom": "16.13.1",
    "reactstrap": "8.5.1",
    "reactstrap-date-picker": "^0.0.11",
    "webpack": "4.44.1"
  }
}
